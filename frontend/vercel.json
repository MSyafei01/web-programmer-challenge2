    {
    "version": 2,
    "name": "web-programmer-challenge",
    "builds": [
        {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "dist"
        }
        },
        {
        "src": "api/**/*.js",
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
        "src": "/api/(.*)",
        "dest": "/api/$1"
        },
        {
        "src": "/(.*)",
        "dest": "/index.html"
        }
    ],
    "env": {
        "JWT_SECRET": "@jwt_secret"
    }
    }